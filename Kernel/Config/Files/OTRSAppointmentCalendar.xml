    <ConfigItem Name="AgentAppointmentAgendaOverview###StripEmptyLines" Required="1" Valid="1">
        <Description Translatable="1">Strips empty lines on the ticket preview in the queue view.</Description>
        <Group>OTRSAppointmentCalendar</Group>
        <SubGroup>Core::AppointmentCalendar::AgendaOverview</SubGroup>
        <Setting>
            <Option SelectedID="0">
                <Item Key="0" Translatable="1">No</Item>
                <Item Key="1" Translatable="1">Yes</Item>
            </Option>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="AgentAppointmentAgendaOverview###ViewAllPossibleTickets" Required="1" Valid="1">
        <Description Translatable="1">Shows all both ro and rw queues in the queue view.</Description>
        <Group>OTRSAppointmentCalendar</Group>
        <SubGroup>Core::AppointmentCalendar::AgendaOverview</SubGroup>
        <Setting>
            <Option SelectedID="0">
                <Item Key="0" Translatable="1">No</Item>
                <Item Key="1" Translatable="1">Yes</Item>
            </Option>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="AgentAppointmentAgendaOverview###HideEmptyQueues" Required="1" Valid="1">
        <Description Translatable="1">Show queues even when only locked tickets are in.</Description>
        <Group>OTRSAppointmentCalendar</Group>
        <SubGroup>Core::AppointmentCalendar::AgendaOverview</SubGroup>
        <Setting>
            <Option SelectedID="0">
                <Item Key="0" Translatable="1">No</Item>
                <Item Key="1" Translatable="1">Yes</Item>
            </Option>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="AgentAppointmentAgendaOverview###HighlightAge1" Required="1" Valid="1">
        <Description Translatable="1">Sets the age in minutes (first level) for highlighting queues that contain untouched tickets.</Description>
        <Group>OTRSAppointmentCalendar</Group>
        <SubGroup>Core::AppointmentCalendar::AgendaOverview</SubGroup>
        <Setting>
            <String Regex="^[0-9]{1,7}$">1440</String>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="AgentAppointmentAgendaOverview###HighlightAge2" Required="1" Valid="1">
        <Description Translatable="1">Sets the age in minutes (second level) for highlighting queues that contain untouched tickets.</Description>
        <Group>OTRSAppointmentCalendar</Group>
        <SubGroup>Core::AppointmentCalendar::AgendaOverview</SubGroup>
        <Setting>
            <String Regex="^[0-9]{1,7}$">2880</String>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="AgentAppointmentAgendaOverview###Blink" Required="0" Valid="1">
        <Description Translatable="1">Activates a blinking mechanism of the queue that contains the oldest ticket.</Description>
        <Group>OTRSAppointmentCalendar</Group>
        <SubGroup>Core::AppointmentCalendar::AgendaOverview</SubGroup>
        <Setting>
            <Option SelectedID="1">
                <Item Key="0" Translatable="1">No</Item>
                <Item Key="1" Translatable="1">Yes</Item>
            </Option>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="AgentAppointmentAgendaOverview###UseSubQueues" Required="0" Valid="1">
        <Description Translatable="1">Include tickets of subqueues per default when selecting a queue.</Description>
        <Group>OTRSAppointmentCalendar</Group>
        <SubGroup>Core::AppointmentCalendar::AgendaOverview</SubGroup>
        <Setting>
            <Option SelectedID="0">
                <Item Key="0" Translatable="1">No</Item>
                <Item Key="1" Translatable="1">Yes</Item>
            </Option>
        </Setting>
