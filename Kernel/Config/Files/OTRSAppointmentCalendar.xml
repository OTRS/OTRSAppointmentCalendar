                    <JavaScript>thirdparty/fullcalendar-3.0.1/fullcalendar.js</JavaScript>
                    <JavaScript>Core.Agent.AppointmentCalendar.js</JavaScript>
                </Loader>
            </FrontendModuleReg>
        </Setting>
