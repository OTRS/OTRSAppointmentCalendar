    <DatabaseInstall Type="post">
        <TableCreate Name="calendar">
            <Column Name="id" Required="true" PrimaryKey="true" AutoIncrement="true" Type="BIGINT"/>
            <Column Name="group_id" Required="true" Type="INTEGER"/>
            <Column Name="name" Required="true" Size="200" Type="VARCHAR"/>
            <Column Name="valid_id" Required="true" Type="SMALLINT"/>
            <Column Name="create_time" Required="true" Type="DATE" />
            <Column Name="create_by" Required="true" Type="INTEGER" />
            <Column Name="change_time" Required="true" Type="DATE" />
            <Column Name="change_by" Required="true" Type="INTEGER" />
            <Unique Name="calendar_id">
                <UniqueColumn Name="id"/>
            </Unique>
            <Unique Name="calendar_name">
                <UniqueColumn Name="name"/>
            </Unique>
            <ForeignKey ForeignTable="groups">
                <Reference Local="group_id" Foreign="id"/>
            </ForeignKey>
            <ForeignKey ForeignTable="valid">
                <Reference Local="valid_id" Foreign="id"/>
            </ForeignKey>
            <ForeignKey ForeignTable="users">
                <Reference Local="create_by" Foreign="id"/>
                <Reference Local="change_by" Foreign="id"/>
            </ForeignKey>
        </TableCreate>
        <TableCreate Name="calendar_appointment">
            <Column Name="id" Required="true" PrimaryKey="true" AutoIncrement="true" Type="BIGINT"/>
            <Column Name="parent_id" Type="BIGINT"/>
            <Column Name="calendar_id" Required="true" Type="BIGINT"/>
            <Column Name="unique_id" Required="true" Size="255" Type="VARCHAR"/>
            <Column Name="title" Required="true" Size="255" Type="VARCHAR"/>
            <Column Name="description" Size="3800" Type="VARCHAR"/>
            <Column Name="location" Size="255" Type="VARCHAR"/>
            <Column Name="start_time" Required="true" Type="DATE"/>
            <Column Name="end_time" Required="true" Type="DATE"/>
            <Column Name="all_day" Type="SMALLINT"/>
            <Column Name="timezone_id" Required="true" Size="100" Type="VARCHAR"/>
            <Column Name="team_id" Type="BIGINT"/>
            <Column Name="resource_id" Size="255" Type="VARCHAR"/>
            <Column Name="recurring" Type="SMALLINT"/>
            <Column Name="recur_freq" Size="100" Type="VARCHAR"/>
            <Column Name="recur_count" Type="INTEGER"/>
            <Column Name="recur_interval" Type="INTEGER"/>
            <Column Name="recur_until" Type="DATE"/>
            <Column Name="recur_byyear" Type="SMALLINT"/>
            <Column Name="recur_bymonth" Type="SMALLINT"/>
            <Column Name="recur_byday" Type="SMALLINT"/>
            <Column Name="create_time" Type="DATE"/>
            <Column Name="create_by" Type="INTEGER"/>
            <Column Name="change_time" Type="DATE"/>
            <Column Name="change_by" Type="INTEGER"/>
            <Unique Name="calendar_appointment_id">
                <UniqueColumn Name="id"/>
            </Unique>
            <ForeignKey ForeignTable="calendar">
                <Reference Local="calendar_id" Foreign="id"/>
            </ForeignKey>
            <ForeignKey ForeignTable="calendar_appointment">
                <Reference Local="parent_id" Foreign="id"/>
            </ForeignKey>
            <ForeignKey ForeignTable="calendar_team">
                <Reference Local="team_id" Foreign="id"/>
            </ForeignKey>
            <ForeignKey ForeignTable="users">
                <Reference Local="create_by" Foreign="id"/>
                <Reference Local="change_by" Foreign="id"/>
            </ForeignKey>
        </TableCreate>
        <TableCreate Name="calendar_appointment_seen">
            <Column Name="id" Required="true" PrimaryKey="true" AutoIncrement="true" Type="BIGINT"/>
            <Column Name="calendar_appointment_id" Type="BIGINT"/>
            <Column Name="user_id" Type="INTEGER"/>
            <Column Name="seen" Type="SMALLINT"/>
            <Unique Name="calendar_appointment_seen_id">
                <UniqueColumn Name="id"/>
            </Unique>
            <ForeignKey ForeignTable="users">
                <Reference Local="user_id" Foreign="id"/>
            </ForeignKey>
        </TableCreate>
        <TableCreate Name="calendar_team">
            <Column Name="id" Required="true" PrimaryKey="true" AutoIncrement="true" Type="BIGINT"/>
            <Column Name="name" Required="true" Size="200" Type="VARCHAR"/>
            <Column Name="group_id" Required="true" Type="INTEGER"/>
            <Column Name="comments" Size="250" Type="VARCHAR"/>
            <Column Name="valid_id" Required="true" Type="SMALLINT"/>
            <Column Name="create_time" Required="true" Type="DATE" />
            <Column Name="create_by" Required="true" Type="INTEGER" />
            <Column Name="change_time" Required="true" Type="DATE" />
            <Column Name="change_by" Required="true" Type="INTEGER" />
            <Unique Name="calendar_team_name">
                <UniqueColumn Name="name"/>
            </Unique>
            <Index Name="calendar_team_group_id">
                <IndexColumn Name="group_id"/>
            </Index>
            <ForeignKey ForeignTable="valid">
                <Reference Local="valid_id" Foreign="id"/>
            </ForeignKey>
            <ForeignKey ForeignTable="groups">
                <Reference Local="group_id" Foreign="id"/>
            </ForeignKey>
            <ForeignKey ForeignTable="users">
                <Reference Local="create_by" Foreign="id"/>
                <Reference Local="change_by" Foreign="id"/>
            </ForeignKey>
        </TableCreate>
        <TableCreate Name="calendar_team_user">
            <Column Name="user_id" Required="true" Type="INTEGER"/>
            <Column Name="team_id" Required="true" Type="INTEGER"/>
            <Column Name="create_time" Required="true" Type="DATE"/>
            <Column Name="create_by" Required="true" Type="INTEGER"/>
            <Column Name="change_time" Required="true" Type="DATE"/>
            <Column Name="change_by" Required="true" Type="INTEGER"/>
            <Unique Name="calendar_team_user_unique">
                <UniqueColumn Name="user_id"/>
                <UniqueColumn Name="team_id"/>
            </Unique>
            <Index Name="calendar_team_team_id">
                <IndexColumn Name="team_id"/>
            </Index>
            <ForeignKey ForeignTable="users">
                <Reference Local="user_id" Foreign="id"/>
                <Reference Local="create_by" Foreign="id"/>
                <Reference Local="change_by" Foreign="id"/>
            </ForeignKey>
        </TableCreate>
        <Insert Table="link_object">
            <Data Key="name" Type="Quote">Appointment</Data>
        </Insert>
    </DatabaseInstall>

    <!-- TODO: Remove after VO testing -->
    <DatabaseUpgrade>
        <TableAlter Name="calendar_appointment" Version="1.0.0.5">
            <ColumnAdd Name="team_id" Type="BIGINT"/>
            <ForeignKeyCreate ForeignTable="calendar_team">
                <Reference Local="team_id" Foreign="id"/>
            </ForeignKeyCreate>
        </TableAlter>
    </DatabaseUpgrade>

    <DatabaseUninstall Type="pre">
        <TableDrop Name="calendar_appointment_seen"/>
        <TableDrop Name="calendar_appointment"/>
        <TableDrop Name="calendar"/>
        <TableDrop Name="calendar_team_user"/>
        <TableDrop Name="calendar_team"/>
    </DatabaseUninstall>
</otrs_package>
